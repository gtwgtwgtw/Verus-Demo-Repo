[Require Synthesized]
nums.len() <= std::i32::MAX as usize
[End Require]

[Ensures Synthesized]
ret >= -1,
if ret >= 0 {
    let idx = ret as usize;
    idx < nums.len() 
    && nums[idx] == target 
    && forall |j: usize| j < idx ==> nums[j] != target
} else {
    forall |j: usize| j < nums.len() ==> nums[j] != target
}
[End Ensures]
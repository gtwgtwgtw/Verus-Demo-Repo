[Require Synthesized] {
    /// The caller must choose n small enough so that all Fibonacci numbers up to index n–1
    /// fit into an i32.
    requires fibo_fits_i32((n as int) - 1);
}
[Ensures Synthesized] {
    /// The returned Vec has length exactly n,
    ensures ret.len() == n;
    /// and its k’th entry is exactly fibo(k).
    ensures forall |k: int|
        0 <= k && k < (n as int) ==>
            (ret[k as usize] as int) == fibo(k);
}